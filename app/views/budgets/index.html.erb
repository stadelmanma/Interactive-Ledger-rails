<p id="notice"><%= notice %></p>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th colspan="3"></th>
        </tr>
    </thead>

    <tbody>
        <% @budgets.each do |budget| %>
            <tr>
                <td><%= budget.name %></td>
                <td><%= link_to 'Show', budget %></td>
                <td><%= link_to 'Edit', edit_budget_path(budget) %></td>
                <td><%= link_to 'Destroy', budget, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
        <% end %>
    </tbody>
</table>

<br>
